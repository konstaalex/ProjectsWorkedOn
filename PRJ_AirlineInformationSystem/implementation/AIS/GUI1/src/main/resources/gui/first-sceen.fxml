<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            prefHeight="500"
            prefWidth="700"
            fx:controller="gui.PrimaryController">
    <center>
        <VBox alignment="CENTER" spacing="10">
            <Button text="Sales Employee" minWidth="150" maxWidth="150" prefWidth="150" onAction="#onSalesEmployeeButtonClick" />
            <Button text="Sales Officer" minWidth="150" maxWidth="150" prefWidth="150" onAction="#onSalesOfficerButtonClick"/>
            <Button text="Sales Manager" minWidth="150" maxWidth="150" prefWidth="150" />
        </VBox>
    </center>

</BorderPane>
